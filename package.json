    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "fs": "0.0.1-security",
    "graphql": "^16.8.1",
    "lucide-react": "^0.542.0",
    "next": "15.4.4",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.6",
    "payload": "3.54.0",
    "postcss": "^8.5.6",
